{
	"extends": [
		"standard"
	],
	"env": {
		"browser": true,
		"es2021": true,
		"jest": true,
		"serviceworker": true
	},
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"no-unused-vars": "warn",
		"no-console": "off",
		"indent": [
			"error",
			2
		],
		"quotes": [
			"error",
			"single"
		],
		"semi": [
			"error",
			"never"
		]
	}
}